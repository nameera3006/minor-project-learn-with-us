<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>TechBlog</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
 
 <!-- to remove .jsp extension from url -->

	<servlet>

		<servlet-name>TechBlog1</servlet-name>
		<jsp-file>/login_page.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
		<servlet-name>TechBlog1</servlet-name>
		<url-pattern>/login</url-pattern>

	</servlet-mapping>
 
 <servlet>
 	<servlet-name>TechBlog2</servlet-name>
		<jsp-file>/register_page.jsp</jsp-file>
	</servlet>

	<servlet-mapping>
    	<servlet-name>TechBlog2</servlet-name>
		<url-pattern>/register</url-pattern>

	</servlet-mapping>
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
  
</web-app>
  